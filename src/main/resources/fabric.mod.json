{
  "schemaVersion": 1,
  "id": "orbital_railgun_enhanced",
  "version": "${version}",
  "name": "Orbital Railgun Enhanced",
  "description": "Destroy your enemies with an orbital strike, but with sounds and more features! This enhanced fork adds custom sound effects, visual shaders, configurable settings, and improved gameplay mechanics. Compatible with Sound Physics Remastered and Dynamic Surroundings.",
  "authors": [
    "KingIronMan2011"
  ],
  "contributors": [
    "Mishkis (Original Orbital Railgun mod)"
  ],
  "contact": {
    "homepage": "https://github.com/KingIronMan2011/orbital-railgun-enhanced",
    "issues": "https://github.com/KingIronMan2011/orbital-railgun-enhanced/issues",
    "sources": "https://github.com/KingIronMan2011/orbital-railgun-enhanced"
  },
  "license": "MIT",
  "icon": "assets/orbital_railgun_enhanced/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.kingironman2011.orbital_railgun_enhanced.client.OrbitalRailgunClient"
    ],
    "main": [
      "io.github.kingironman2011.orbital_railgun_enhanced.OrbitalRailgun"
    ],
    "owo": [
      "io.github.kingironman2011.orbital_railgun_enhanced.client.config.EnhancedConfig"
    ]
  },
  "mixins": [
    "orbital_railgun_enhanced.mixins.json",
    {
      "config": "orbital_railgun_enhanced.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "fabric": "*",
    "minecraft": ">=1.20 <=1.20.1",
    "java": ">=17",
    "owo": ">=${owolib_version}",
    "geckolib": ">=4.0"
  },
  "recommends": {
    "modmenu": ">=7.0.0"
  },
  "suggests": {
    "sound_physics_remastered": "*",
    "dynamicsurroundings": "*"
  },
  "breaks": {
    "optifabric": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.source": "https://github.com/KingIronMan2011/orbital-railgun-enhanced",
        "modmenu.issues": "https://github.com/KingIronMan2011/orbital-railgun-enhanced/issues"
      }
    },
    "loom:injected_interfaces": {},
    "mc-publish": {
      "loaders": ["fabric", "quilt"],
      "game-versions": ["1.20", "1.20.1"],
      "dependencies": {
        "fabric-api": { "type": "required" },
        "owo-lib": { "type": "required" },
        "geckolib": { "type": "required" }
      }
    }
  }
}
